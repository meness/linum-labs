{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!javascriptreact",
    "!jupyter",
    "!latex",
    "!less",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala",
    "!scss",
    "!swift",
    "!vue",
    "!dockercompose",
    "dotenv",
    "git-rebase",
    "!graphql",
    "ignore",
    "log",
    "!makefile",
    "!shellscript",
    "svg",
    "tailwindcss",
    "toml",
    "typescript"
  ],
  "cSpell.language": "en-US",
  "cSpell.words": [
    "CODEOWNERS",
    "comfortaa",
    "commitlint",
    "consts",
    "cssnano",
    "dtos",
    "endregion",
    "Eskandari",
    "filepond",
    "flexbox",
    "heroicons",
    "hookform",
    "Ipfs",
    "meness",
    "musharka",
    "mypinata",
    "nextjs",
    "nextui",
    "onaddfile",
    "onupdatefiles",
    "packagejson",
    "Parens",
    "rainbowkit",
    "sepolia",
    "stylelint",
    "svgr",
    "tailwindcss",
    "tanstack",
    "todos",
    "viem",
    "wagmi",
    "walletconnect",
    "walletlink"
  ],
  "css.validate": false,
  "editor.autoClosingQuotes": "always",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "eslint.lintTask.enable": true,
  "eslint.probe": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.vscode": true,
    "**/node_modules": true,
    "**/.next": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.next/**": true
  },
  "git.autoStash": true,
  "git.autofetch": true,
  "git.closeDiffOnOperation": true,
  "git.enableSmartCommit": true,
  "git.repositoryScanIgnoredFolders": [
    "node_modules",
    ".next",
    ".husky",
    ".vscode",
    ".next",
    "typings",
    "public",
    "src"
  ],
  "githubIssues.createIssueTriggers": ["TODO", "BUG", "FIX"],
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.ignoredPullRequestBranches": [
    "main"
  ],
  "i18n-ally.displayLanguage": "en-US",
  "i18n-ally.enabledFrameworks": ["react-i18next"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "public/locales",
  "i18n-ally.namespace": true,
  "i18n-ally.readonly": false,
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.usage.derivedKeyRules": ["{key}_other", "{key}_one", "{key}_zero"],
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.documentSelectors": [
    "**/*.svg"
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true
  },
  "tailwindCSS.files.exclude": ["**/.git/**", "**/node_modules/**", "**/.hg/**", "**/.next/**"],
  "todo-tree.filtering.excludeGlobs": ["node_modules", ".next"],
  "todo-tree.general.automaticGitRefreshInterval": 60,
  "todo-tree.general.tags": ["BUG", "FIX", "TODO"],
  "todo-tree.highlights.backgroundColourScheme": ["red", "orange", "blue"],
  "todo-tree.highlights.foregroundColourScheme": ["white", "white", "white"],
  "todo-tree.highlights.useColourScheme": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsdk": "node_modules/typescript/lib"
}
